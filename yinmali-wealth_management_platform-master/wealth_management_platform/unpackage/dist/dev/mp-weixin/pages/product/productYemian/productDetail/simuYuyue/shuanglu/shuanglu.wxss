@import "../../../../../../common/vendor.wxss";

page {
  background: rgb(255, 255, 255);
}
.shuanglu-box {
  background: #f5f5f5;
  padding-top: 20rpx;
}
.shuanglu {
  background: rgb(255,255,255);
  padding: 0 30rpx;
}
.title {
  text-align: center;
  line-height: 100rpx;
  font-size: 17px;
}
.row {
  height: 98rpx;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.left {
  font-size: 15px;
  margin-right: 20px;
}
.questionBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  width: 100%;
  padding: 40rpx 0;
  text-align: center;
  position: absolute;
  bottom: 0;
  right: 0;
  left: 0;
}
.question {
  width: 240rpx;
  height: 70rpx;
  background: -webkit-linear-gradient(right, rgba(196, 156, 90, 1) 0%, rgba(226, 196, 138, 1) 100%);
  background: linear-gradient(-90deg, rgba(196, 156, 90, 1) 0%, rgba(226, 196, 138, 1) 100%);
  border-radius: 35px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  color: rgb(255,255,255);
}
._label {
  margin-right: 10rpx;
}
.shuoming {
  width: 650rpx;
  padding: 18rpx;
  color: rgba(153,153,153,1);
  font-size: 14px;
  height: 100rpx;
  border: 1px solid rgba(238,238,238,1);
  border-radius: 5px;
  margin-top: 20rpx;
}
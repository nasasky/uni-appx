@import "../../../../../../common/vendor.wxss";

page {
  background: rgb(255, 255, 255);
}
.renzheng-box {
  padding: 30rpx;
}
.title {
  font-size: 17px;
  text-align: center;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 30rpx;
}
.ceshi-box {
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  line-height: 34rpx;
}
.row1 {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding: 38rpx 0 45rpx;
}
.radio {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  font-weight: 500;
  margin-right: 70rpx;
}
.radio image {
  height: 16px;
  width: 16px;
  margin-right: 10rpx;
}
.paste-box {
  position: relative;
}
.paste-content {
  font-size: 13px;
  width: 100%;
  height: 100rpx;
}
.paste {
  width: 140rpx;
  height: 22rpx;
  border: 1px solid rgba(196, 156, 90, 1);
  border-radius: 11px;
  text-align: center;
  padding: 10rpx;
  line-height: 22rpx;
  color: rgba(196, 156, 90, 1);
  position: absolute;
  right: 0;
  bottom: -20rpx;
}
.blank {
  height: 160rpx;
  margin-top: 50rpx;
  border: 1px solid rgba(238, 238, 238, 1);
  border-radius: 5px;
}
.row {
  height: 98rpx;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.left {
  font-size: 15px;
  margin-right: 20px;
}
.btn-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.btn {
  padding: 0 30rpx;
  height: 70rpx;
  background: -webkit-linear-gradient(right, rgba(196, 156, 90, 1) 0%, rgba(226, 196, 138, 1) 100%);
  background: linear-gradient(-90deg, rgba(196, 156, 90, 1) 0%, rgba(226, 196, 138, 1) 100%);
  border-radius: 35rpx;
  color: rgb(255,255,255);
  text-align: center;
  line-height: 70rpx;
  font-size: 14px;
}
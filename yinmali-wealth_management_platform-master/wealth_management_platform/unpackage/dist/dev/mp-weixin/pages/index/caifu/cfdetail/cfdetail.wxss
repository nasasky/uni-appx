@import "../../../../common/vendor.wxss";

page {
  background: white;
}
.topBox {
  width: 100%;
  height: 370rpx;
  background: url("http://ai.ylcaifu.com/appimg/images/7.0-%E7%99%BB%E5%BD%95%E8%AE%A4%E8%AF%81%E7%AD%89%E9%A1%B5%E9%9D%A2/7.1.3-%E5%AF%86%E7%A0%81%E9%87%8D%E7%BD%AE/android/drawable-xhdpi/beijing.png") center no-repeat;
  background-size: 100% 370rpx;
}
.top {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding: 67rpx 30rpx 37rpx 37rpx;
  color: rgb(255, 255, 255);
}
.middle {
  font-size: 18px;
}
.go-back {
  font-size: 17px;
}
.share {
  font-size: 20px;
}
.videoBox {
  padding: 0 30rpx;
}
.videoBox .video {
  width: 100%;
  border-radius: 10rpx;
}
.contentBox {
  padding: 0 30rpx 110rpx;
  margin-top: 230rpx;
}
.contentTop {
  padding: 30rpx 0;
  border-bottom: 1px solid rgba(238, 238, 238, 1);
}
.title {
  font-size: 15px;
}
.bot {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  font-size: 11px;
  line-height: 48rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  color: rgba(153, 153, 153, 1);
  font-weight: 500;
}
.left,
.right {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.left .from {
  margin-right: 10rpx;
}
.right {
  color: rgba(196, 156, 90, 1);
  font-size: 12px;
}
.uni-font17 {
  font-size: 17px;
}
.contentMid {
  padding: 0 0 56rpx 0;
  border-bottom: 1px solid rgba(238, 238, 238, 1);
}
.videoDetail {
  border-left: 4px solid rgba(203, 147, 89, 1);
  padding-left: 20rpx;
  margin: 49rpx 0 28rpx 0;
  color: rgba(51, 51, 51, 1);
  font-weight: bold;
}
.detail {
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  line-height: 34rpx;
}
.mark,
.mark-top-left,
.mark-right .mark-top {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
}
.mark {
  margin-top: 29rpx;
  padding-bottom: 37rpx;
  border-bottom: 1px solid rgba(238, 238, 238, 1);
}
.mark-top-left,
.mark-right .mark-top {
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}
.mark-top-left {
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
  font-size: 16px;
}
.avator-left,
.avator-left image {
  height: 80rpx;
  width: 80rpx;
  border-radius: 50%;
}
.mark-right {
  margin-left: 19rpx;
}
.position {
  margin-right: 10rpx;
}
.mark-bot {
  font-size: 14px;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 42rpx;
  margin-top: 14rpx;
}
.mark-right .time {
  font-size: 12px;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}
.more-mark {
  text-align: center;
  font-weight: 500;
  color: rgba(196, 156, 90, 1);
  line-height: 82rpx;
}
._label {
  margin-right: 10rpx;
}
.bottom {
  width: 100%;
  position: fixed;
  bottom: 0;
}